@model EazyWizy.WebUI.Models.ContentModel
@{
    Layout = null;
    ViewBag.Title = "Online Grocery Mall";
}
<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>EazyWizy.com</title>
        @Styles.Render("~/Content/Site/css")    
        @Styles.Render("~/Content/kendo/css") 
        @Scripts.Render("~/bundles/jquery")  
        @Scripts.Render("~/bundles/kendo")   
        @Scripts.Render("~/bundles/Site")   
    </head>
    <body>
        <div id="divRoot" style="height:600px;background-image:url(/Content/Images/WoodDivBackground.jpg);background-repeat:repeat;">
           <div id="divRootInner">   
             <div id="TopMenu">            
                 @Html.Partial("Menu/_TopMenuPartial")               
             </div>  
             <div class="disp-Block div-mid-top"></div>
             <div class="disp-Block">
                 @Html.Partial("Menu/_PartialLHSMenu",Model.lhsAllMenu)      
             </div>             
           </div>
        </div>      
    </body>
</html>

